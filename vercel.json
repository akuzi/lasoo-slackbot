{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/events",
      "dest": "/api/events.js"
    },
    {
      "src": "/api/commands",
      "dest": "/api/commands.js"
    },
    {
      "src": "/api/interactions",
      "dest": "/api/interactions.js"
    }
  ]
}

